<div class="well">
  <h1>Register new tenant</h1>
  <form id="form-quick-registration" data-parsley-validate>
    <div class="row">
      <div class="col-md-4">
        <input type="text" class="form-control input-lg" placeholder="Your business name" required>
      </div>
      <div class="col-md-3">
        <input type="text" class="form-control input-lg" placeholder="Email address">
      </div>
      <div class="col-md-2">
        <input type="password" class="form-control input-lg" placeholder="Password">
      </div>
      <div class="col-md-3">
      <input type="submit" class="btn btn-success btn-lg btn-block" value="Get Started">
      </div>
    </div>
  </form>
</div>